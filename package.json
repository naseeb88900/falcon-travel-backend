{
  "name": "falcontour",
  "version": "1.0.0",
  "description": "Falcon Tour Backend - TypeScript + Express + TypeORM + PostgreSQL",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -d src/config/db.ts",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/config/db.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d src/config/db.ts",
    "dev": "cross-env nodemon --watch src --ext ts --exec ts-node src/index.ts",
    "dev:vercel": "vercel dev",
    "build": "tsc -p tsconfig.json",
    "vercel-build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "deploy": "vercel",
    "deploy:prod": "vercel --prod"
  },
  "engines": {
    "node": ">=18 <23"
  },
  "keywords": [],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "apple-auth": "^1.0.9",
    "apple-signin-auth": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.7.0",
    "dotenv": "^17.0.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "google-auth-library": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nanoid": "3.3.7",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/stripe": "^8.0.416",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.25",
    "typescript": "^5.4.5"
  }
}
